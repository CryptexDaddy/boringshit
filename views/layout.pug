doctype html
html
  head
    title= title
    block css
      link(rel='stylesheet', href='/stylesheets/style.css')
      link(rel='stylesheet', href='/stylesheets/normform.min.css')
      link(rel='stylesheet', type='text/css', href='/semantic/semantic.min.css')
  body(onload='kek()')
    header
      nav
        include nav.pug
    .ui.two.column.relaxed.grid
      .row
        .two.wide.column
          include sidenav.pug
        .fourteen.wide.column
          main
            block content

           

      
    footer
      block footer
        
        button.circular.ui.icon.button.right.huge.liveSupportBlob
          i.icon.comments
    script.
    
    block js
      script(src='https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.0/jquery.min.js')
      script(src='/semantic/semantic.js')
      script(src='/javascripts/main.js')
      script(src='https://kit.fontawesome.com/5bbbb0823d.js', crossorigin='anonymous')
          